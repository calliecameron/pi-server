      <script type="text/javascript">
        document.write('<a class="navbar-brand" href="http://' + window.location.hostname + '/"><img class="logo-image d-inline-block" src="http://' + window.location.hostname + '/favicon.png">{{ body_title }}</a>');
      </script>
